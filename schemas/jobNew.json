{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companyNew.schema.json",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "minimum": 1
        },
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 60
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "string",
            "minLength": 1,
            "maxLength": 3
        },
        "companyHandle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        }
    },
    "additionalProperties": false,
    "required": ["title", "salary", "equity", "companyHandle"]
}
